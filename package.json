{
  "name": "odk-central-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=production watchify -vd -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e lib/main.js -o dist/build.js",
    "lint": "eslint --ext .js,.vue lib test",
    "test": "karma start",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e lib/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "dependencies": {
    "axios": "~0.17",
    "bootstrap": "~3",
    "jquery": "~3",
    "luxon": "~1",
    "pako": "~1.0",
    "pluralize": "~7.0",
    "qrcode-generator": "~1.3",
    "ramda": "~0",
    "vue": "~2.5",
    "vue-router": "~3.0",
    "vuex": "~3.1"
  },
  "devDependencies": {
    "avoriaz": "~6.3",
    "babel-core": "~6.26",
    "babel-plugin-transform-runtime": "~6.23",
    "babel-preset-env": "~1.6",
    "babel-preset-stage-2": "~6.24",
    "babel-runtime": "~6.26",
    "babelify": "~7.3",
    "browserify": "~13.3",
    "cached-path-relative": "~1.0.2",
    "cross-env": "~2.0",
    "envify": "~3.4",
    "eslint": "~4",
    "eslint-config-airbnb-base": "~12.1",
    "eslint-plugin-import": "~2.8",
    "eslint-plugin-vue": "~4",
    "faker": "~4.1",
    "jsqr": "~1.0",
    "karma": "~3.0",
    "karma-browserify": "~5.3",
    "karma-chrome-launcher": "~2.2",
    "karma-mocha": "~1.3",
    "karma-spec-reporter": "0.0.32",
    "lodash": "~4.17.11",
    "mocha": "~5.2",
    "node-sass": "~4.9",
    "proxyquireify": "~3.2",
    "should": "~13.2",
    "uglify-js": "~2.8",
    "vueify": "~9.4",
    "watchify": "~3.11"
  }
}
